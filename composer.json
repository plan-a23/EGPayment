{
    "name": "plan-a23/egpayment",
    "description": "",
    "keywords": [
        "plan-a23",
        "eg payment",
        "online payments",
        "paypal",
        "paymob",
        "kashier",
        "hyperpay",
        "fawry"
    ],
    "homepage": "https://github.com/plan-a23/egpayment",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ahmed eid",
            "email": "ahmed.m.eid.2001@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0",
        "laravel/framework": ">=9.0",
        "paypal/paypal-checkout-sdk": "1.0.2",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "PlanA23\\EGPayment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PlanA23\\EGPayment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PlanA23\\EGPayment\\EGPaymentServiceProvider"
            ],
            "aliases": {
                "EGPayment": "PlanA23\\EGPayment\\EGPaymentFacade"
            }
        }
    }
}
